section .text
          global _start

_start:
          mov eax, 65

label:
          push eax
          mov [res], eax
          mov ecx, res
          call output
          call linefeed
          pop eax
          inc eax
          cmp eax, 90
          jle label
          call exit

linefeed:
          mov eax, 10
          mov [res], eax
          mov ecx, res
          call output
          ret

output:
          mov edx, 1
          mov ebx, 1
          mov eax, 4
          int 0x80
          ret

exit:
          mov eax, 1
          int 0x80

segment .bss
          res resb 1
