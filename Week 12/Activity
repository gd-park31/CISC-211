  GNU nano 6.2                                                  w12.asm                                                        
_oddeven:
        push ebp
        mov ebp, esp
        sub esp, 4
        mov eax, dword[ebp+4]
        and eax, 1
        jz evnn

odd:
        mov edx, 1
        mov ebx, 1
        mov ecx, 'o'
        mov eax, 4
        int 0x80

evnn:
        mov edx,1       ;output length
        mov ebx,1       ;stdout
        mov ecx, 'e'
        mov eax,4       ;system call (sys_write)
        int 0x80        ;interrupt kernel

        leave
        ret

section .text
        global _start

_start:
        push 24
        call _oddeven

        mov ebx, 0
        mov eax, 1
        int 0x80
