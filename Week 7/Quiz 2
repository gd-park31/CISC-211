section .text
        global _start

_start:
        mov eax, [var1]         ;assign var1 to eax register
        mov ebx, [var2]         ;assign var2 to ebx register
        mul ebx                 ;multiply ebx to eax
        mov [result1], eax

        mov ecx, [var3]         ;assign var3 to ecx register
        mov edx, [var4]         ;assign var4 to edx register
        mul edx                 ;multiply edx to
        mov [result2], ecx      ;save the value within ecx to result2

        add [result1], [result2]        ;add saved value from result2 to result1

        mov eax, 1
        int 0x80

section .bss
        result1 resb 1
        result2 resb 1

section .data
        var1 dd 1
        var2 dd 2
        var3 dd 3
        var4 dd 4
